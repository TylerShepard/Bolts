async function startProgram() {
await scrollMatrixText('START', { r: 255, g: 0, b: 0 }, 30, true)
await roll(0,80,2)
setMainLed ({r: 0, g: 0, b: 255})
await roll (90,34,0.27)
await roll (90,57,2)
await roll (170,69,1)
await Sound.Animal.play(true)
await roll (-150,70,1.5)
setMainLed ({r: 255, g: 0, b: 0})
await roll (90,57,.5)
} 



